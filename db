CREATE TABLE `datenbanken2`.`dfbmitglieder` ( `id` INT NOT NULL AUTO_INCREMENT , `vorname` VARCHAR(255) NOT NULL , `nachname` VARCHAR(255) NOT NULL , `mitglied` BOOLEAN NOT NULL , `email` VARCHAR(255) NOT NULL , PRIMARY KEY (`id`)) ENGINE = InnoDB;
CREATE TABLE `datenbanken2`.`newsgroup` ( `liga` VARCHAR(255) NOT NULL , PRIMARY KEY (`liga`)) ENGINE = InnoDB;
CREATE TABLE `datenbanken2`.`whichliga` ( `dfb_mitglied` INT NOT NULL , `liga` VARCHAR(255) NOT NULL , UNIQUE `id` (`dfb_mitglied`, `liga`)) ENGINE = InnoDB; 
